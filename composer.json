{
    "name": "jiny/permit",
    "description": "permit for JinyPHP",
    "keywords": [
        "jinyPHP",
        "framwork",
        "permit"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://www.jinyphp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Permit\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Permit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Permit\\JinyPermitServiceProvider"
            ]
        }
    }
}
